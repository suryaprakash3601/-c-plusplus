#include<iostream>
using namespace std;
int main(){
    int a = 123;
    cout << a << endl;
    char b = 'v';
    cout<< b << endl;
    bool b1 = true;
    cout<< b1 << endl;
    float f = 1.23;
    cout<< f << endl;
    double d = 1.23;
    cout<< d << endl;
    int size = sizeof(d);
    cout<< "Size of a is : " << size << endl;
}
